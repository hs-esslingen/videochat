{
  "name": "videochat",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "watch-debug": "concurrently npm:watch-frontend npm:watch-server npm:watch-build-server",
    "build": "npm run build-frontend && npm run build-server",
    "test": "ng test",
    "lint": "gts check && tsc --noEmit",
    "e2e": "ng e2e",
    "start": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker node dist/server.js",
    "build-frontend": "ng build --prod",
    "build-server": "webpack --config webpack.server.config.js",
    "watch-build-server": "webpack -w --config webpack.server.config.js",
    "watch-server": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker nodemon --watch dist/server.js --inspect dist/server.js",
    "watch-frontend": "ng serve",
    "prerender": "ng run videochat:prerender",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc",
    "test-compile": "tsc --noEmit",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run check"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test-compile"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "private": true,
  "dependencies": {
    "@nguniversal/express-engine": "^9.1.0",
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.2.1",
    "mediasoup": "^3.5.6",
    "mediasoup-client": "^3.6.4",
    "node-fetch": "^2.6.0",
    "nodemailer": "^6.4.6",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-saml": "^1.3.3",
    "ws": "^7.2.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.901.0",
    "@angular/animations": "~9.1.0",
    "@angular/cdk": "^9.2.0",
    "@angular/cli": "~9.1.0",
    "@angular/common": "~9.1.0",
    "@angular/compiler": "~9.1.0",
    "@angular/compiler-cli": "~9.1.0",
    "@angular/core": "~9.1.0",
    "@angular/forms": "~9.1.0",
    "@angular/language-service": "~9.1.0",
    "@angular/material": "^9.2.0",
    "@angular/platform-browser": "~9.1.0",
    "@angular/platform-browser-dynamic": "~9.1.0",
    "@angular/platform-server": "~9.1.0",
    "@angular/router": "~9.1.0",
    "@nguniversal/builders": "^9.1.0",
    "@nguniversal/express-engine": "^9.1.0",
    "@nguniversal/module-map-ngfactory-loader": "8.2.6",
    "@types/express": "^4.17.4",
    "@types/express-session": "^1.17.0",
    "@types/jasmine": "~3.5.10",
    "@types/jasminewd2": "~2.0.8",
    "@types/jsonwebtoken": "^8.3.8",
    "@types/node": "^13.11.1",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-saml": "^1.1.1",
    "@types/ws": "^7.2.3",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.1.0",
    "copy-webpack-plugin": "^5.1.1",
    "cross-env": "^7.0.2",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "jasmine-core": "~3.5.0",
    "jasmine-spec-reporter": "~5.0.1",
    "karma": "~4.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~2.1.1",
    "karma-jasmine": "~3.1.1",
    "karma-jasmine-html-reporter": "^1.5.3",
    "node-loader": "^0.6.0",
    "nodemon": "^2.0.2",
    "parse5": "^5.1.1",
    "protractor": "~5.4.3",
    "rxjs": "~6.5.5",
    "ts-loader": "^6.2.2",
    "ts-node": "~8.8.1",
    "tslib": "^1.11.1",
    "tslint": "~6.1.1",
    "typescript": "~3.8.3",
    "webpack-cli": "^3.3.11",
    "zone.js": "~0.10.3"
  }
}
