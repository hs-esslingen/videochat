{
  "name": "videochat",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "ng": "ng",
    "watch-debug": "concurrently npm:watch-frontend npm:watch-server npm:watch-build-server",
    "build": "npm run build-frontend && npm run build-server",
    "lint": "gts check",
    "start": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker node dist/server.js",
    "build-frontend": "ng build --prod",
    "build-server": "webpack --config webpack.server.config.js",
    "watch-build-server": "webpack -w --config webpack.server.config.js",
    "watch-server": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker nodemon --watch dist/server.js --inspect dist/server.js",
    "watch-frontend": "ng serve",
    "check": "gts check",
    "clean": "gts clean",
    "test-compile": "tsc --noEmit",
    "fix": "gts fix"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test-compile"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@angular/animations": "~10.0.1",
    "@angular/cdk": "^10.0.0",
    "@angular/common": "~10.0.1",
    "@angular/compiler": "~10.0.1",
    "@angular/core": "~10.0.1",
    "@angular/forms": "~10.0.1",
    "@angular/language-service": "~10.0.1",
    "@angular/localize": "^10.0.3",
    "@angular/material": "^10.0.0",
    "@angular/platform-browser": "~10.0.1",
    "@angular/platform-browser-dynamic": "~10.0.1",
    "@angular/router": "~10.0.1",
    "body-parser": "^1.19.0",
    "connect-redis": "^4.0.4",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "linkifyjs": "^2.1.9",
    "log4js": "^6.3.0",
    "mediasoup": "^3.6.8",
    "mediasoup-client": "^3.6.10",
    "node-fetch": "^2.6.0",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "passport-jwt": "^4.0.0",
    "passport-oauth2": "^1.5.0",
    "passport-saml": "^1.3.3",
    "redis": "^3.0.2",
    "rxjs": "~6.5.5",
    "ws": "^7.3.0",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1000.2",
    "@angular/cli": "~10.0.2",
    "@angular/compiler-cli": "~10.0.3",
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.6",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "~3.5.11",
    "@types/jasminewd2": "~2.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/nodemailer": "^6.4.0",
    "@types/passport": "^1.0.3",
    "@types/passport-azure-ad": "^4.0.7",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/passport-saml": "^1.1.2",
    "@types/ws": "^7.2.5",
    "codelyzer": "^5.2.2",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "gts": "^2.0.2",
    "husky": "^4.2.5",
    "node-loader": "^1.0.0",
    "nodemon": "^2.0.4",
    "parse5": "^6.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "~8.10.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.2",
    "typescript": "~3.9.5",
    "webpack-cli": "^3.3.12"
  }
}
