{
  "name": "videochat",
  "version": "0.0.0",
  "license": "GPL-3.0",
  "scripts": {
    "watch-debug-gu": "concurrently npm:watch-frontend-gu npm:watch-server npm:watch-build-server",
    "watch-debug-hse": "concurrently npm:watch-frontend-hse npm:watch-server npm:watch-build-server",
    "watch-debug-hse-gu": "concurrently npm:watch-frontend-hse npm:watch-frontend-gu npm:watch-server npm:watch-build-server",
    "watch-frontend-gu": "ng serve --configuration gu",
    "watch-frontend-hse": "ng serve --configuration hse --port 4201",
    "watch-build-server": "webpack -w --config webpack.server.config.js",
    "watch-server": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker nodemon --watch dist/server.js --inspect dist/server.js",
    "build": "npm run build-frontend-gu && npm run build-server",
    "build-hse": "npm run build-frontend-hse && npm run build-server",
    "build-gu": "npm run build-frontend-gu && npm run build-server",
    "build-frontend-gu": "ng build --prod --localize --configuration=\"production,production-gu\"",
    "build-frontend-hse": "ng build --prod --localize --configuration=\"production,production-hse\"",
    "build-server": "webpack --config webpack.server.config.js",
    "start": "cross-env MEDIASOUP_WORKER_BIN=dist/worker/mediasoup-worker node dist/server.js",
    "check": "gts lint",
    "clean": "gts clean",
    "test-compile": "tsc --noEmit",
    "lint": "gts lint && tsc --noEmit",
    "fix": "gts fix",
    "ng": "ng"
  },
  "husky": {
    "hooks": {
      "pre-push": "npm run lint && npm run test-compile"
    }
  },
  "engines": {
    "node": ">=10.0.0"
  },
  "dependencies": {
    "@angular/animations": "~11.0.2",
    "@angular/cdk": "^11.0.1",
    "@angular/common": "~11.0.2",
    "@angular/compiler": "~11.0.2",
    "@angular/core": "~11.0.2",
    "@angular/forms": "~11.0.2",
    "@angular/language-service": "~11.0.2",
    "@angular/localize": "^11.0.2",
    "@angular/material": "^11.0.1",
    "@angular/platform-browser": "~11.0.2",
    "@angular/platform-browser-dynamic": "~11.0.2",
    "@angular/router": "~11.0.2",
    "body-parser": "^1.19.0",
    "connect-redis": "^5.0.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "helmet": "^4.1.1",
    "jsonwebtoken": "^8.5.1",
    "linkifyjs": "^2.1.9",
    "log4js": "^6.3.0",
    "mediasoup": "^3.6.22",
    "mediasoup-client": "^3.6.16",
    "negotiator": "^0.6.2",
    "node-fetch": "^2.6.1",
    "passport": "^0.4.1",
    "passport-azure-ad": "^4.3.0",
    "passport-jwt": "^4.0.0",
    "passport-oauth2": "^1.5.0",
    "passport-saml": "^1.3.3",
    "redis": "^3.0.2",
    "rxjs": "~6.6.3",
    "ws": "^7.3.1",
    "zone.js": "~0.10.3"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^0.1100.2",
    "@angular/cli": "~11.0.2",
    "@angular/compiler-cli": "~11.0.2",
    "@types/connect-redis": "0.0.14",
    "@types/express": "^4.17.8",
    "@types/express-session": "^1.17.0",
    "@types/helmet": "0.0.48",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.8",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/linkifyjs": "^2.1.3",
    "@types/negotiator": "^0.6.1",
    "@types/node": "^14.0.14",
    "@types/node-fetch": "^2.5.7",
    "@types/passport": "^1.0.4",
    "@types/passport-azure-ad": "^4.0.7",
    "@types/passport-jwt": "^3.0.3",
    "@types/passport-oauth2": "^1.4.9",
    "@types/passport-saml": "^1.1.2",
    "@types/ws": "^7.2.5",
    "concurrently": "^5.2.0",
    "copy-webpack-plugin": "^6.0.2",
    "cross-env": "^7.0.2",
    "gts": "^3.0.2",
    "husky": "^4.2.5",
    "jasmine": "^3.6.3",
    "karma": "^5.2.3",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage-istanbul-reporter": "^3.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-html-reporter": "^1.5.4",
    "node-loader": "^1.0.0",
    "nodemon": "^2.0.4",
    "parse5": "^6.0.0",
    "ts-loader": "^7.0.5",
    "ts-node": "~8.10.2",
    "tslib": "^2.0.0",
    "tslint": "~6.1.2",
    "typescript": "~4.0.5",
    "webpack-cli": "^3.3.12"
  }
}
